{
    "name": "diselabsdev/callcenter-integration",
    "description": "Call Center Integration Module for Magento 2",
    "type": "magento2-module",
    "version": "1.0.0",
    "license": [
        "MIT"
    ],
    "require": {
        "php": "~8.1.0|~8.2.0",
        "magento/framework": "^103.0",
        "magento/module-backend": "^103.0",
        "magento/module-customer": "^103.0",
        "magento/module-sales": "^103.0",
        "magento/module-catalog": "^103.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "CallCenter\\Integration\\": ""
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Diselabsdev/callcenter"
        }
    ],
    "support": {
        "issues": "https://github.com/Diselabsdev/callcenter/issues",
        "source": "https://github.com/Diselabsdev/callcenter"
    },
    "keywords": [
        "magento2",
        "call-center",
        "integration",
        "customer-service"
    ]
} 